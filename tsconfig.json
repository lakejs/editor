{
  "compilerOptions": {
    "outDir": "temp",
    "moduleResolution": "node",
    "target": "es6",
    "allowJs": false,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true,
    "removeComments": false,
    "esModuleInterop": true
  }
}
